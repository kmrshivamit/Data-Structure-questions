{
  public static void main (String[]args)
  {int arr[]={23,33,3,2,43};
    System.out.println (search(arr,7,arr.length-1));
  }

  static boolean search (int arr[], int item, int index)
  { 
      if(index<0)
      return false;
      
      if(arr[index]==item)
      return true;
      else
      return search(arr,item,--index);
      
      

  }
}
